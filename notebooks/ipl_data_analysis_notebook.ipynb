   "source": [
    "# Season-Wise Toss Analysis\n",
    "season_toss_analysis = (\n",
    "    matches_cleaned_df.groupBy(\"season\", \"toss_winner\", \"toss_decision\")\n",
    "    .agg(\n",
    "        count(\"*\").alias(\"Total_matches\"),\n",
    "        count(when(col(\"toss_winner\") == col(\"winner\"), True)).alias(\"Wins\"),\n",
